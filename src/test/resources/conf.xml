<suite name="UI tests" verbose="1">
    <tests>
        <test name = "test">
            <!--To run test by classes-->
            <classes>
                <class name="com.google.GoogleSearchTest" />
            </classes>
        </test>
        <!--To run tests by packages-->
        <test name="package">
            <packages>
                <package name="w3schools" />
            </packages>
        </test>
        <test name="Windows + FireFox Test">
            <parameters>
                <parameter name="platform" value="SIERRA" />
                <parameter name="remoteBrowser" value="firefox" />
            </parameters>
            <classes>
                <class name="com.google.GoogleSearchTest" />
            </classes>
        </test>
    </tests>
</suite>